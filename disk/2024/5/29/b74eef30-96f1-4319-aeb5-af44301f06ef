select * from stock_product where stock_id ='da75203a-0d6a-41d9-8bd4-015790fe552c'

select * from (
select d1.id, d1.quantity, d1.quantity_out ,
(select sum(quantity) from stock_product where status =0 and rel_id = d1.id)  ,
d4.name, d2.receipt_date, d5.create_date, d2.id, d1.product_id, d3.status, d6.receipt_date, d6.id, d1.unit_id
from stock_product d1 
left outer join stock d2 on(d1.stock_id = d2.id)
left outer join product d3 on(d1.product_id = d3.id)
left outer join stock_warehouse d4 on(d1.warehouse_id = d4.id)
left outer join stock_product d5 on(d1.id = d5.rel_id and d5.status =0)
left outer join stock d6 on(d5.stock_id = d6.id)
where d1.status =0 and d2.status =0
--and 
--d2.receipt_date<='2024-01-02 23:59:59'
--and d1.warehouse_id='9a99a1bf-610f-4622-e144-7050fbda8d47'
--and d3.code='TSNHE005'
order by  d5.create_date asc
) d where d.quantity_out != 0 AND d.quantity_out != d.sum

