left outer join sale_order d2 on(d1.order_id = d2.id)
left outer join customer_register d3 on(d2.rel_id = d3.id)
where d2.customer_id ='b830741f-791f-49d4-ad54-1c260027de98'
and d1.status =0


select d1.id, d4.code, d1.write_date, d1.conformity_code, d3.receipt_no, d6.parent_id from sale_order_product d1
left outer join sale_order d2 on(d1.order_id = d2.id)
left outer join customer_register d3 on(d2.rel_id = d3.id)
left outer join product_private d4 on(d1.private_id = d4.id)
left outer join customer_register_line d5 on(d1.register_line_id =d5.id)
left outer join product_private d6 on(d5.rel_id = d6.id)
where d2.customer_id ='b830741f-791f-49d4-ad54-1c260027de98'
and d1.status =0  and d1.write_date>'2023-07-12 00:00:00' 
order by d4.code, d1.conformity_code, d1.write_date



