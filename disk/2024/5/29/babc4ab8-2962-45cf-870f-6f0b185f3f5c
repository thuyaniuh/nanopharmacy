update sale_order_product set status =0 where id in(
select d1.id from sale_order_product d1
left outer join sale_order d2 on(d1.order_id = d2.id)
left outer join customer_register d3 on(d2.rel_id = d3.id)
where d3.receipt_no='VICB.1631' and d1.status =14)


