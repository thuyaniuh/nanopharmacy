select d1.id, d3.receipt_no, d1.publish, d1.status, d1.create_date, d1.write_date, d1.conformity_code, d4.code, d4.name from sale_order_product d1
left outer join sale_order d2 on(d1.order_id = d2.id)
left outer join customer_register d3 on(d2.rel_id = d3.id)
left outer join product_private d4 on(d1.private_id = d4.id)
where d3.receipt_no ='VICB.1740' and d1.publish =1
order by  d1.conformity_code asc

select d1.id, d1.status, d3.receipt_no, d1.publish, d1.status, d1.create_date,
 d1.write_date, d1.conformity_code, 
d4.code, d4.name 
, d4.version, d1.scheme_id,d1.address
from sale_order_product d1
left outer join sale_order d2 on(d1.order_id = d2.id)
left outer join customer_register d3 on(d2.rel_id = d3.id)
left outer join product_private d4 on(d1.private_id = d4.id)
where d3.receipt_no ='VICB.1868' and d1.publish =1
and 
(d1.conformity_code ='VICB 0797.10-08'
or d1.conformity_code ='VICB 0797.10-14'
or d1.conformity_code ='VICB 0797.10-198'
or d1.conformity_code ='VICB 0797.10-206'
)
order by  d1.conformity_code asc

