SELECT DISTINCT d5.id AS receipt_id, d5.receipt_no, d5.receipt_date, d7.id AS id4, d7.name AS warehouse_name4, 
d4.id AS workorder_id,d8.version 
FROM stock_warehouse_rel d1 
LEFT OUTER JOIN stock_warehouse d2 ON(d1.warehouse_id = d2.id)
 LEFT OUTER JOIN stock_warehouse d3 ON(d2.parent_id = d3.id) 
 LEFT OUTER JOIN mrp_workorder d4 ON(d1.rel_id = d4.id AND d4.status =0) 
 LEFT OUTER JOIN res_receipt d5 ON(d4.rel_id = d5.id) 
 LEFT OUTER JOIN stock_warehouse d6 ON(d3.parent_id = d6.id) 
 LEFT OUTER JOIN stock_warehouse d7 ON(d6.parent_id = d7.id) LEFT OUTER JOIN res_version d8 ON(d5.id = d8.rel_id) 
 WHERE d7.id IS NOT NULL AND d1.status =0 AND d2.status =0 AND d5.res_id ='1d9de800-17f3-4eea-c7a9-46e3d2ef1da8' 
 AND (d5.status =2 OR d5.status=3)  and d5.receipt_date>='2024-03-09 00:00:00' 
AND d5.receipt_date<='2024-03-09 23:59:59.999' ORDER BY d5.receipt_date ASC, d5.id ASC

