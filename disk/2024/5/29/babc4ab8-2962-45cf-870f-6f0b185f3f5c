select d1.id, d1.status, d1.quantity, d1.quantity_out, (select sum(n.quantity) from stock_product n
left outer join stock m on(n.stock_id = m.id) where n.rel_id = d1.id and n.status =0 and m.status =0) as qty_out, d4.quantity, d4.id
, d5.status, d5.id
from stock_product d1
left outer join stock d2 on(d1.stock_id = d2.id)
left outer join product d3 on(d1.product_id = d3.id)
left outer join stock_product d4 on(d1.rel_id = d4.id)
left outer join stock_product d5 on(d1.id = d5.rel_id)
where d1.status =0 and d1.warehouse_id ='a42d8c7a-6061-4965-d85c-e6ef66160969'
and d3.code='VLPEE039'
and d2.receipt_date<='2023-09-04 00:59:59'
--and d2.receipt_date>='2023-08-01 23:59:59'
--and d1.quantity_out =0
order by d1.rel_id asc

