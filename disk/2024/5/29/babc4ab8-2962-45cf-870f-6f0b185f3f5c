select d3.id, d3.status, d1.id, d1.status,  d7.name,d1.receipt_no,  d3.quantity, d3.quantity_out, d4.name, d6.receipt_no, d6.receipt_date from stock d1
left outer join res_user d2 on(d1.create_uid = d2.id)
left outer join stock_product d3 on(d1.id = d3.stock_id)
left outer join stock_warehouse d4 on(d3.warehouse_id = d4.id)
left outer join stock_product d5 on(d3.id = d5.rel_id)
left outer join stock d6 on(d5.stock_id = d6.id)
left outer join product d7 on(d3.product_id = d7.id)
where d3.status =0 and d1.receipt_no='NTP2412697'
or d1.receipt_no='NTP2412707'
order by d1.receipt_no ASC

